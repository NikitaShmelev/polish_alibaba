<main>
  
  

  <header class="p-3 bg-dark text-white">
    <div class="container">
      <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
        <a href="/" class="d-flex align-items-center mb-2 mb-lg-0 text-white text-decoration-none">
          <svg class="bi me-2" width="40" height="32" role="img" aria-label="Bootstrap"><use xlink:href="#bootstrap"/></svg>
        </a>

        <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
          <li>
              <a href="<%= root_path %>" class="nav-link text-white">
                <svg class="bi d-block mx-auto mb-1" width="24" height="24"><use xlink:href="#home"/></svg>
                Home
              </a>
          </li>

          <% if current_campaign %>
          <li>
            <a href="#" class="nav-link text-white">
              <svg class="bi d-block mx-auto mb-1" width="24" height="24"><use xlink:href="#speedometer2"/></svg>
                Dashboard
            </a>
          </li>
          <li>
              <a href="#" class="nav-link text-white">
              <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="currentColor" class="bi d-block mx-auto mb-1" viewBox="0 0 16 16">
                <path d="M2.678 11.894a1 1 0 0 1 .287.801 10.97 10.97 0 0 1-.398 2c1.395-.323 2.247-.697 2.634-.893a1 1 0 0 1 .71-.074A8.06 8.06 0 0 0 8 14c3.996 0 7-2.807 7-6 0-3.192-3.004-6-7-6S1 4.808 1 8c0 1.468.617 2.83 1.678 3.894zm-.493 3.905a21.682 21.682 0 0 1-.713.129c-.2.032-.352-.176-.273-.362a9.68 9.68 0 0 0 .244-.637l.003-.01c.248-.72.45-1.548.524-2.319C.743 11.37 0 9.76 0 8c0-3.866 3.582-7 8-7s8 3.134 8 7-3.582 7-8 7a9.06 9.06 0 0 1-2.347-.306c-.52.263-1.639.742-3.468 1.105z"/>
              </svg>
              Chats
            </a>
          </li>
          
          <% elsif current_user %>
            <% if current_user.admin? %>
              <a href="<%= user_url(current_user) %>" class="nav-link text-white">
              <svg class="bi d-block mx-auto mb-1" width="24" height="24"><use xlink:href="#speedometer2"/></svg>
                Admin dashboard
              </a>
            <% end %>
          <% else %>
            <a href="#" class="nav-link text-white">
              <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="currentColor" class="bi d-block mx-auto mb-1" viewBox="0 0 16 16">
                <path d="M2.678 11.894a1 1 0 0 1 .287.801 10.97 10.97 0 0 1-.398 2c1.395-.323 2.247-.697 2.634-.893a1 1 0 0 1 .71-.074A8.06 8.06 0 0 0 8 14c3.996 0 7-2.807 7-6 0-3.192-3.004-6-7-6S1 4.808 1 8c0 1.468.617 2.83 1.678 3.894zm-.493 3.905a21.682 21.682 0 0 1-.713.129c-.2.032-.352-.176-.273-.362a9.68 9.68 0 0 0 .244-.637l.003-.01c.248-.72.45-1.548.524-2.319C.743 11.37 0 9.76 0 8c0-3.866 3.582-7 8-7s8 3.134 8 7-3.582 7-8 7a9.06 9.06 0 0 1-2.347-.306c-.52.263-1.639.742-3.468 1.105z"/>
              </svg>
              Chats
            </a>
          <% end %>
          <li>
              <a href="<%= orders_path %>" class="nav-link text-white">
                <svg class="bi d-block mx-auto mb-1" width="24" height="24"><use xlink:href="#table"/></svg>
                Orders
              </a>
          </li>
          <li>
            <% if current_campaign %>
              <a href="<%= products_path %>" class="nav-link text-white">

                <svg class="bi d-block mx-auto mb-1" width="24" height="24"><use xlink:href="#grid"/></svg>
                  Products
              </a>
            <% elsif %>
              <a href="<%= cart_url(1) %>" class="nav-link text-white">

                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="currentColor" class="bi d-block mx-auto mb-1" viewBox="0 0 16 16">
                <path d="M0 1.5A.5.5 0 0 1 .5 1H2a.5.5 0 0 1 .485.379L2.89 3H14.5a.5.5 0 0 1
                          .491.592l-1.5 8A.5.5 0 0 1 13 12H4a.5.5 0 0 1-.491-.408L2.01 3.607 1.61 
                          2H.5a.5.5 0 0 1-.5-.5zM3.102 4l1.313 7h8.17l1.313-7H3.102zM5 12a2 2 0 1 0 0 4 2 
                          2 0 0 0 0-4zm7 0a2 2 0 1 0 0 4 2 2 0 0 0 0-4zm-7 1a1 1 0 1 1 0 2 1 1 0 0 1 0-2zm7 
                          0a1 1 0 1 1 0 2 1 1 0 0 1 0-2z"/>
                </svg>
                Cart
              </a>
            <% end %>
                
                
            </a>
          </li>
          <% if current_user %>
            <li>
            <a href="<%= user_url(current_user.id)%>" class="nav-link text-white">
              <svg class="bi d-block mx-auto mb-1" width="24" height="24"><use xlink:href="#people-circle"/></svg>
              Profile
            </a>
          </li>
          <% end %>
          
        </ul>

        <form class="col-12 col-lg-auto mb-3 mb-lg-0 me-lg-3">
          <input type="search" class="form-control form-control-dark" placeholder="Search..." aria-label="Search">
        </form>
          <% if current_user %>
            <div class="text-end" id='first'>
              <button class="btn btn-outline-light me-2" id='dropbtn'>Welcome <%= current_user.first_name %></button>
              <div class="dropdown-content">
                <%= link_to 'Logout', destroy_user_session_path, method: :delete, class: 'btn btn-outline-light me-2', type: 'button'%>
                <%= link_to 'Cart', user_url(current_user.id), class: 'btn btn-outline-light me-2', type: 'button'%>
              </div>
            </div>
          <% elsif current_campaign %>
            <div class="text-end" id='first'>
              <button class="btn btn-outline-light me-2" id='dropbtn'>Welcome <%= current_campaign.campaign_name %></button>
                <div class="dropdown-content">
                  <%= link_to 'Campaign Profile', campaign_url(current_campaign.id), class: 'btn btn-outline-light me-2', type: 'button'%>
                  <%= link_to 'Logout', destroy_campaign_session_path, method: :delete, class: 'btn btn-outline-light me-2', type: 'button'%>
                </div>
            </div>
          <% else %>
            <div class="text-end" id='first'>
              <button class="btn btn-outline-light me-2" id='dropbtn'>Log in</button>
                <div class="dropdown-content">
                  <%= link_to 'Customer', new_user_session_path, class: 'btn btn-outline-light me-2', type: 'button'%>
                  <%= link_to 'Campaign', new_campaign_session_path, class: 'btn btn-warning', type: 'button' %>
                </div>
            </div>
            <div class="text-end" id='second'>
              <button class="btn btn-warning" id='dropbtn'>Sign up</button>
                <div class="dropdown-content">
                  <%= link_to 'Campaign', new_campaign_registration_path, class: 'btn btn-outline-light me-2', type: 'button'%>
                  <%= link_to 'Customer', new_user_registration_path, class: 'btn btn-warning', type: 'button' %>
                </div>
            </div>
          <% end %>
            
       
          
        
        
      </div>
    </div>
  </header>


  
</main>


    <script src="/docs/5.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-/bQdsTh/da6pkI1MST/rWKFNjaCP5gBSY4sEBT38Q/9RBh9AH40zEOg7Hlq2THRZ" crossorigin="anonymous"></script>
  <p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>
  <%= yield %>
  </body>
</html>

    

